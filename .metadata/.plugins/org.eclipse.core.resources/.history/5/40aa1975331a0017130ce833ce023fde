package AppServer.Netwok;

import java.io.DataOutputStream;
import java.io.UnsupportedEncodingException;

public class SendString {
	public static void sendData(String str, DataOutputStream dos) throws UnsupportedEncodingException{
		byte[] byteData = str.getBytes("8859_1");
		dos.write(byteData), 0, byteData.length);
	}
}
